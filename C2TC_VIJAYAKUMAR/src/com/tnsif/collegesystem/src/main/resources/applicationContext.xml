<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- Define CollegeService Bean with id "collegeService" -->
    <bean id="collegeService" class="com.tns.collegesystem.service.CollegeService" />

    <!-- Define StudentService Bean with id "studentService" -->
    <bean id="studentService" class="com.tns.collegesystem.service.StudentService" />

    <!-- Another example of CollegeService Bean without id -->
    <bean class="com.tns.collegesystem.service.CollegeService" />
    
    <bean class="com.tns.collegesystem.service.CollegeService" />
	
	<bean class="com.tns.collegesystem.service.CollegeService" />

</beans>
